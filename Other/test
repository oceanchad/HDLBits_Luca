    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x7ff138015ed0_0;
    %store/vec4 v0x7ff138016020_0, 0, 8;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x7ff138016020_0;
    %addi 1, 0, 8;
    %store/vec4 v0x7ff138016020_0, 0, 8;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7ff138005a20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ff138016220_0, 0, 1;
T_1.0 ;
